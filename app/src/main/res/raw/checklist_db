CREATE TABLE "Categories_by_Location" (`_id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, `Category_id`	INTEGER NOT NULL, `Location_id`	INTEGER NOT NULL, `Remove`	INTEGER DEFAULT 0, FOREIGN KEY(`Category_id`) REFERENCES Categories (_id ), FOREIGN KEY(`Location_id`) REFERENCES Locations (_id ));
CREATE TABLE `Categories_by_Area` (`_id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, `Category_id`	INTEGER NOT NULL, `Area_id`	INTEGER NOT NULL, FOREIGN KEY(`Category_id`) REFERENCES Categories(_id), FOREIGN KEY(`Area_id`) REFERENCES Areas(_id));
